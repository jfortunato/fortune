{
    "name": "fortune/fortune",
    "authors": [
        {
            "name": "Justin Fortunato",
            "email": "jfortunato25@gmail.com"
        }
    ],
    "require": {
        "jms/serializer": "~0.16",
        "vlucas/valitron": "~1.1.7"
    },
    "require-dev": {
        "behat/behat": "2.4.*@stable",
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "~4.0",
        "guzzlehttp/guzzle": "~4.0",
        "doctrine/orm": "~2.4",
        "doctrine/data-fixtures": "~1.0",
        "slim/slim": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Fortune\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/_bootstrap"],
        "psr-4": {
            "Fortune\\Test\\": "tests/acceptance/app-bootstrap/src/",
            "test\\Fortune\\": "tests/integration/phpunit/"
        }
    }
}
